{
  "mcpServers": {
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/mnt/d/Cursos/VectorSeek",
        "/mnt/d/Cursos/VectorSeekFrontend",
        "/mnt/d/Cursos/alurafake-main",
        "/mnt/d/Cursos/prospectos"
      ],
      "env": {}
    },
    "git": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-git",
        "/mnt/d/Cursos/VectorSeek",
        "/mnt/d/Cursos/VectorSeekFrontend",
        "/mnt/d/Cursos/alurafake-main",
        "/mnt/d/Cursos/prospectos"
      ],
      "env": {}
    },
    "docs-mcp-server": {
      "transport": "sse",
      "url": "http://localhost:6280/sse",
      "disabled": false,
      "autoApprove": []
    },
    "lsp": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-v",
        "/mnt/d/Cursos:/work:rw",
        "-w",
        "/work",
        "docker.io/jonrad/lsp-mcp:0.3.1"
      ],
      "env": {}
    }
  }
}
